msgstr "    -C# [oder --cylinders #]:  Zahl der Zylinder setzen"

#: fdisk/sfdisk.c:2319
msgid "    -H# [or --heads #]:    set the number of heads to use"
msgstr "    -H# [oder --heads #]:      Zahl der Köpfe setzen"

